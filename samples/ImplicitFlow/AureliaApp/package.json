  },
  "peerDependencies": {},
  "devDependencies": {
    "aurelia-cli": "^1.0.0-beta.13",
    "aurelia-tools": "^0.2.2",
    "browser-sync": "^2.13.0",
    "connect-history-api-fallback": "^1.2.0",
    "event-stream": "^3.3.3",
    "gulp": "^4.0.0",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-notify": "^2.2.0",
